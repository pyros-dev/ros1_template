<!--
  Standard launch file. for more information check http://wiki.ros.org/roslaunch/XML.
 -->
<launch>

  <!-- Args -->
  <arg name="config_override"        default="$(find ros1_template)/config/override.yaml" doc="used by upper launchers to customize configuration"/>

  <!-- Launch ROS template node -->
  <node pkg="ros1_template" type="template_node.py" name="template">
    <rosparam command="load" file="$(find ros1_template)/config/default.yaml" />
    <rosparam command="load" file="$(arg config_override)" />
  </node>

</launch>
