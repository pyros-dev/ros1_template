<!--
  Standard launch file. for more information check http://wiki.ros.org/roslaunch/XML.
 -->
<launch>

  <!-- Args -->
  <arg name="config_override"        default="$(find ros1_template)/config/oracle-override.yaml" doc="used by upper launchers to customize configuration"/>

  <!-- Launch oracle node -->
  <node pkg="ros1_template" type="oracle_node.py" name="oracle">
    <rosparam command="load" file="$(find ros1_template)/config/oracle-default.yaml" />
    <rosparam command="load" file="$(arg config_override)" />
  </node>

</launch>
